{
  "name": "node-express-project-starter",
  "version": "1.0.0",
  "description": "Project starter for developing RESTful APIs using Nodejs, Express and MongoDB.",
  "main": "server.js",
  "scripts": {
    "start": "node ./index.js",
    "dev": "nodemon ./index.js",
    "test": "npm run lint && npm run test-unit",
    "test-watch": "npm run test-unit -- --watch",
    "test-unit": "NODE_ENV=test mocha test/setup.spec.js 'lib/**/*.spec.js' --exit",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexsalomon/node-express-project-starter.git"
  },
  "author": "Alex Salomon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexsalomon/node-express-project-starter/issues"
  },
  "homepage": "https://github.com/alexsalomon/node-express-project-starter#readme",
  "devDependencies": {
    "@strv/eslint-config-javascript": "^9.1.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "sinon": "^7.1.1"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
