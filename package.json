{
  "name": "node-exp-api-boilerplate",
  "version": "1.0.0",
  "description": "Project starter for developing RESTful APIs using Nodejs, Express and MongoDB.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexsalomon/node-exp-api-boilerplate.git"
  },
  "author": "Alex Salomon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexsalomon/node-exp-api-boilerplate/issues"
  },
  "homepage": "https://github.com/alexsalomon/node-exp-api-boilerplate#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "dev": "NODE_ENV=dev nodemon --require pretty-error/start ./index.js",
    "test": "npm run lint && npm run tests",
    "test-watch": "npm run tests -- --watch",
    "tests": "NODE_ENV=test nyc mocha --recursive test/setup.spec.js source/ --exit",
    "lint": "eslint source/** test/** --fix",
    "docs": "apidoc -f '.*\\.js$' -i source/ -o docs/"
  },
  "nyc": {
    "exclude": [
      "test/**",
      "**/*.spec.js",
      "**/*.docs.js",
      "build"
    ]
  },
  "devDependencies": {
    "@strv/eslint-config-javascript": "^9.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-sorted": "^0.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "pretty-error": "^2.1.1",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "convict": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "express-authorize": "^1.2.0",
    "express-validation": "^1.0.2",
    "fs-extra": "^7.0.1",
    "helmet": "^3.15.0",
    "http-status": "^1.3.1",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "supertest": "^3.3.0",
    "winston": "^3.1.0",
    "winston-sentry-raven-transport": "^1.0.2"
  }
}
